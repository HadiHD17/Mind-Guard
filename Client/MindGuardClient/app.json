{
  "expo": {
    "name": "MindGuard",
    "slug": "mindguard",
    "scheme": "mindguard",
    "owner": "hadi17",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#80C6DA"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hadi.mindguard"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#80C6DA"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hadi.mindguard"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      [
        "expo-calendar",
        {
          "calendarPermission": "MindGuard needs calendar access to add your routines."
        }
      ],
      "expo-secure-store"
    ]
  }
}
